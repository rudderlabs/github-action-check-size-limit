{
  "name": "github-action-check-size-limit",
  "version": "2.7.0",
  "private": true,
  "description": "size-limit action",
  "main": "dist/index.js",
  "license": "ISC",
  "scripts": {
    "format": "prettier --write **/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "build": "ncc build src/main.ts -o dist",
    "size-build": "npm run build",
    "size": "npm run size-build && size-limit"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "4500 ms"
    }
  ],
  "dependencies": {
    "@actions/core": "1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "6.0.0",
    "bytes": "3.1.2",
    "has-pnpm": "1.1.1",
    "has-yarn": "3.0.0",
    "markdown-table": "3.0.3"
  },
  "devDependencies": {
    "@size-limit/file": "10.0.1",
    "@size-limit/time": "10.0.1",
    "@size-limit/webpack": "10.0.1",
    "@types/jest": "29.5.6",
    "@types/node": "18.18.7",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vercel/ncc": "0.38.1",
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-github": "4.10.1",
    "eslint-plugin-jest": "27.4.3",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "29.7.0",
    "jest-circus": "29.7.0",
    "js-yaml": "4.1.0",
    "prettier": "3.0.3",
    "size-limit": "10.0.1",
    "ts-jest": "29.1.1",
    "typescript": "5.2.2"
  }
}
